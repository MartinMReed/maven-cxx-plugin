			<configuration>
				<lifecycles>
					<lifecycle>
						<id>default</id>
						<phases>
							<validate>org.hardisonbrewing:maven-cxx-plugin:validate</validate>
							<initialize>org.hardisonbrewing:maven-cxx-plugin:initialize</initialize>
