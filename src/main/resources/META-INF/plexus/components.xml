							<prepare-package>org.hardisonbrewing:maven-cxx-plugin:xcode-generate-ipa,
								org.hardisonbrewing:maven-cxx-plugin:xcode-prepare-package</prepare-package>
							<package>org.hardisonbrewing:maven-cxx-plugin:package</package>
							<install>org.apache.maven.plugins:maven-install-plugin:install</install>
